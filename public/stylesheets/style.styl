/*
 * CSS stylesheet generated from "style.styl" using Stylus preprocessor.
 */

body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif

a
  color: #00B7FF

.node
  fill: #ccc

.node circle
  stroke #fff
  stroke-width: 1.5px
  opacity: 1.0

.node text
  fill: black;
  pointer-events: none

line
  fill: none
  stroke: #666
  stroke-width: 1.5px
  stroke-opacity: 1.0